<%- include header %>
	<div class="text-center page-title">新用户注册</div>
	<div class ="container">
		<div class="row">
			<div class="col-xs-4 col-xs-offset-4">
				<form method="post">
					<div class="form-group">
						<input type="text" class="form-control" name ="username"  placeholder="<% if(tip==1){ %>用户名不符合规则<%  }else if(yonghu==1){  %>用户名已经注册<%  }else{ %>用户名<%  } %> ">
					</div>
					<div class="form-group">
						<input type="password" class="form-control" name ="password" placeholder="<% if(tip1==1){ %>输入密码不符合规则<% }else{ %>密码<% } %>" >
					</div>
					<div class="form-group">
						<input type="password" class="form-control" name ="passwordRepeat" placeholder="确认密码">
					</div>
					<button type="submit" class="btn btn-default center-block">注册</button>

				</form>

			</div>
		</div>
	</div>
					
<%- include footer %>